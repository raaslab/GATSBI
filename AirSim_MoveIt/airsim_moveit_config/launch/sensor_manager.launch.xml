<launch>

  <!-- This file makes it easy to include the settings for sensor managers -->

  <!-- Params for the octomap monitor -->
  <!--  <param name="octomap_frame" type="string" value="some frame in which the robot moves" /> -->
  <param name="octomap_frame" type="string" value="world_enu" />
  <param name="octomap_resolution" type="double" value="1" />
  <param name="max_range" type="double" value="40.0" />

  <!-- Load the robot specific sensor manager; this sets the moveit_sensor_manager ROS parameter -->
  <arg name="moveit_sensor_manager" default="quadrotor" />
  <include file="$(find airsim_moveit_config)/launch/$(arg moveit_sensor_manager)_moveit_sensor_manager.launch.xml" />

</launch>
